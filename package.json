  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "yarn run clean && yarn run transpile",
    "transpile": "bin/transpile",
    "clean": "rm -rf es lib",
    "extract": "format-message extract -g underscored_crc32 --out-file translations/en.json 'src/**/*.js'",
    "lint": "eslint .",
    "start": "webpack-dev-server",
    "test": "karma start",
    "test:headless": "karma start --headless --no-sandbox",
    "mocha": "find src -name '*.js' | grep '__tests__' | xargs mocha --compilers js:@babel/register --require src/test/mocha-bootstrap",
    "snyk-monitor": "node --max-old-space-size=4096 node_modules/.bin/snyk monitor --org=instructure --file=yarn.lock --project-name=outcomes-service:node",
    "snyk-test": "node --max-old-space-size=4096 node_modules/.bin/snyk test --org=instructure --file=yarn.lock --project-name=outcomes-service:node",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "jest .storybook/storyshots.test.js"
  },
